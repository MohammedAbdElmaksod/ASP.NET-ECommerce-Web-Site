@model List<TbProduct>

<section class="bg-light" id="portfolio">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="text-uppercase section-heading">Shop</h2>
                <h3 class="text-muted section-subheading">welcome to our Shop</h3>
            </div>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {
                <partial name="_PartialShop" model="item" />
            }
        </div>
    </div>
</section>